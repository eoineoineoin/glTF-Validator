                        },
                        {
                            "angularAxes": [0],
                            "max": 1.0
                        }
                    ],
                    "drives": [
                        {
                            "type": "linear",
                            "mode": "force",
                            "axis": 0,
                            "maxForce": 1,
                            "velocityTarget": 2,
                            "damping": 3,
                            "positionTarget": 4,
                            "stiffness": 5
